<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="assetic.filter.sassrewrite.class">Jhg\SassBridgeBundle\Assetic\Filter\SassRewriteFilter</parameter>
    </parameters>

    <services>
        <service id="assetic.filter.sassrewrite" class="%assetic.filter.sassrewrite.class%">
            <tag name="assetic.filter" alias="sassrewrite"></tag>
        </service>
    </services>
</container>